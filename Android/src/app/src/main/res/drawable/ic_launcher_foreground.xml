<vector xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:aapt="http://schemas.android.com/aapt"
    android:width="108dp"
    android:height="108dp"
    android:viewportWidth="108"
    android:viewportHeight="108">
    <!-- Layered geometric design inspired by the provided image -->
    <!-- Bottom layer - green -->
    <path android:pathData="M20,70 L88,70 L88,88 L20,88 Z"
        android:fillColor="#4CAF50"
        android:fillAlpha="0.8" />
    
    <!-- Middle layer - gradient from green to pink -->
    <path android:pathData="M25,60 L83,60 L83,78 L25,78 Z">
        <aapt:attr name="android:fillColor">
            <gradient
                android:startX="25"
                android:startY="60"
                android:endX="83"
                android:endY="78"
                android:type="linear">
                <item
                    android:color="#4CAF50"
                    android:offset="0.0" />
                <item
                    android:color="#FF8A80"
                    android:offset="1.0" />
            </gradient>
        </aapt:attr>
    </path>
    
    <!-- Top layer - pink/coral -->
    <path android:pathData="M30,50 L78,50 L78,68 L30,68 Z"
        android:fillColor="#FF8A80"
        android:fillAlpha="0.9" />
    
    <!-- Inner pyramid/triangle shape -->
    <path android:pathData="M54,35 L65,55 L43,55 Z"
        android:fillColor="#FFFFFF"
        android:fillAlpha="0.8" />
</vector>